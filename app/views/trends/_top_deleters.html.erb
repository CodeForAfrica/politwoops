<h2><%= trend.name.gsub('_', ' ').capitalize %></h2>
<table class="table table-condensed table-stripped table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th>Deleted</th>
  </tr>
  </thead>
  <tbody>
  <% JSON.parse(trend.value).each do |name, value| %>
  <tr>
    <td>
      <%= link_to name, politician_path(name) %>
    </td>
    <td>
      <%= value %>
    </td>
  </tr>
  <% end %>
  </tbody>
</table>


